{
  "name": "npName",
  "short_name": "npShort",
  "start_url": "./",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#ffffff",
  "icons": [
    {
      "sizes": "192x192",
      "src": "192x192.png",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "./index.html",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "text": "text",
      "title": "title",
      "url": "url",
      "files": [
        {
          "name": "textFiles",
          "accept": [
            "text/plain",
            ".txt"
          ]
        },
        {
          "name": "htmlFiles",
          "accept": [
            "text/html",
            ".html"
          ]
        },
        {
          "name": "images",
          "accept": [
            "image/jpeg",
            "image/png",
            "image/webp",
            "image/gif"
          ]
        }
      ]
    }
  },
  "prefer_related_applications": false,
  "scope": ".",
  "description": "npDesc"
}